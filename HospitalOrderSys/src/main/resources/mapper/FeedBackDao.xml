<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.sfturing.dao.FeedBackDao">

	<insert id="inserFeedBack">
		INSERT INTO `feed_back`
		(user_id,content)
		VALUE
		(#{userId},#{content})
	</insert>

</mapper>