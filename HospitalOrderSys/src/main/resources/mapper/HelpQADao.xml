<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.sfturing.dao.HelpQADao">
	<!-- 通过ID地区信息 -->
	<select id="findQAByType" resultType="cn.sfturing.entity.HelpQA"
		parameterType="java.lang.String">
		SELECT
		* FROM `help_q_a`
		WHERE question_type = #{questionype}
	</select>
	

</mapper>